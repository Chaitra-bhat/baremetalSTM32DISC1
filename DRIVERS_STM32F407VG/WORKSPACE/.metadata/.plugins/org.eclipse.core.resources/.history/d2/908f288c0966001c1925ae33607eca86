#include<gpio.h>

//INITILIZATION AND DEINITILIZATION
void GPIO_Init(void)
{

}

void GPIO_DeInit(void)
{

}


//PERIPHERAL CLOCK ENABLE AND DISBALE
void GPIO_PeriClockEnable(GPIOHandle_t *GPIOHandle, uint8_t EnaOrDis)
{
	if(GPIOHandle->pGPIOx == GPIOA)
	{
		GPIOA_PCLK_ENABLE;
	}
	else
	{
		GPIOA_PCLK_DISABLE;
	}
}


