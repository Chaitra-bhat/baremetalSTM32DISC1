#include<spi.h>

void SPI_PeriClkEnable(SPI_RegDef_t *pSPIx, uint8_t EnaOrDis)
{
	if(EnaOrDis == ENABLE)
	{
		if(pSPIx == SPI1)
			SPI1_PCLK_ENABLE();
		else if(pSPIx == SPI2)
			SPI2_PCLK_ENABLE();
		else if(pSPIx == SPI3)
			SPI3_PCLK_ENABLE();
	}
}

void delay(uint32_t delayVal)
{
	for(int i =0; i<delayVal;i++);
}

